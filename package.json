{
  "name": "learning-ext",
  "displayName": "Learning ext",
  "version": "0.1.0",
  "description": "A basic Plasmo extension.",
  "author": "Ocean Bao",
  "scripts": {
    "start": "next start",
    "dev": "run-p dev:*",
    "dev:plasmo": "plasmo dev",
