    "@tailwindcss/typography": "^0.5.8",
    "next": "13.0.7",
    "plasmo": "0.60.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.1",
    "@plasmohq/rps": "1.8.1",
    "@plasmohq/storage": "0.13.1",
    "@types/chrome": "0.0.204",
    "@types/node": "18.11.15",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "postcss": "^8.4.20",
    "prettier": "2.8.1",
    "typescript": "4.9.4"
